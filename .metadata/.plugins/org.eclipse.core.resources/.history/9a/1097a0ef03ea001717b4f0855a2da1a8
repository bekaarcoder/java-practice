package Practice;

import java.util.ArrayList;

public class ArrayListPractice {

	public static void main(String[] args) {
		ArrayList words = new ArrayList();
		words.add("hello");
		words.add("world");
		words.add(12);
		
		Object item1 = words.get(0);
		Object item2 = words.get(1);
		
		System.out.println(item1 + "-" + item2);
	}

}
